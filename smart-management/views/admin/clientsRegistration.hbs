<div class="box-clientsRegistration">
  <div class="movap-titulo">
    <h4 align="left">CADASTRO DE CLIENTES</h4>
    <hr>
  </div>

  <form action="/clientsRegistration" class="col-10" method="post">
    <div class="form-group col-8">
      <p class="movap-nomes" align="left">Código do Cliente</p>
      <input type="text" class="form-control" id="intentificação" name="Clients[codClient]"placeholder="">
    </div>
    <div class="form-group col-8">
      <p class="movap-nomes" align="left">Razão Social</p>
      <input type="text" class="form-control" id="intentificação" name="Clients[corporateName]"placeholder="">
    </div>
    <div class="form-group col-8">
      <p class="movap-nomes" align="left">Nome Fantasia</p>
      <input type="text" class="form-control" id="intentificação" name="Clients[fantasyname]"placeholder="">
    </div>
    <div class="form-group col-8">
      <p class="movap-nomes" align="left">CNPJ</p>
      <input type="number" class="form-control" id="intentificação" name="Clients[cnpj]"placeholder="">
    </div>

    <div class="form-row">
      <div class="form-group col-6">
        <p class="movap-nomes" align="left" for="inputAddress">Endereço</p>
        <input type="text" class="form-control" id="inputAddress" placeholder="" name="Clients[address][street]">
      </div>
      <div class="form-group col-1">
        <p class="movap-nomes" align="left">Número</p>
        <input type="number" class="form-control" name="Clients[address][number]">
      </div>
    </div>
    <div class="form-row">
      <div class="form-group col-4">
        <label for="inputCity">Cidade</label>
        <input type="text" class="form-control" id="inputCity" name="Clients[address][city]">
      </div>
      <div class="form-group col-2">
        <label for="inputEstado">Estado</label>
        <select id="inputEstado" class="form-control">
          <option selected>Escolher...</option>
          <option>AC</option>
          <option>AL</option>
          <option>AP</option>
          <option>AM</option>
          <option>BA</option>
          <option>CE</option>
          <option>DF</option>
          <option>ES</option>
          <option>GO</option>
          <option>MA</option>
          <option>MT</option>
          <option>MS</option>
          <option>MG</option>
          <option>PA</option>
          <option>PB</option>
          <option>PR</option>
          <option>PE</option>
          <option>PI</option>
          <option>RJ</option>
          <option>RN</option>
          <option>RS</option>
          <option>RO</option>
          <option>RR</option>
          <option>SC</option>
          <option>SP</option>
          <option>SE</option>
          <option>TO</option>
        </select>
      </div>
      <div class="form-group col-2">
        <label for="inputCEP">CEP</label>
        <input type="number" class="form-control" id="inputCEP" name="Clients[address][cep]">
      </div>
    </div>
    <div class="form-group col-8">
      <p class="movap-nomes" align="left">E-mail</p>
      <input type="email" class="form-control" id="intentificação" name="Clients[email]" placeholder="">
    </div>
    <div class="form-group col-8">
      <p class="movap-nomes" align="left">Telefone</p>
      <input type="tel" class="form-control" id="intentificação" name="Clients[phone]"placeholder="">
    </div>
    <div class="form-group col-8">
      <p class="movap-nomes" align="left">Nome para contato</p>
      <input type="text" class="form-control" id="intentificação" name="Clients[nameContact]"placeholder="">
    </div>
    <div class="form-group col-8">
      <p class="movap-nomes" align="left">Status</p>
      <select class="form-control" id="exampleFormControlSelect1" name="Clients[status]">
        <option>Ativo</option>
        <option>Outros</option>
      </select>
    </div>
    <div class="form-group justify-content-start col-md-8 d-flex movap">
      <input type="submit" value="CONFIRMAR" class="btn btn-primary" >
    </div>
  </form>
</div>
