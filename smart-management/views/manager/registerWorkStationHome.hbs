<div class="movap-titulo">
  <h4 align="left">LISTA DE ATENDENTES</h4>
  <script src="/javascripts/dashboard.js"></script>
  <script src="/javascripts/jquery.js" charset="utf-8"></script>
  <hr>
</div>
<div class="col">
  <div class="row">
    <div class="col-5">
      <div class="row">
        <div class="input-group">
          <div class="btn_new">
          <button type="button" class="btn btn-primary btn-info btn-secondary" aria-haspopup="true"
            aria-expanded="false" /><a style="color:#ffffff" href="/station/signup">NOVO</a></button>
            </div>
          <input id="myInput" class="form-control" type="text" onkeyup="searchW();" placeholder="Pesquisar"
            aria-label="Search">
          <div class="input-group-append">
            <button class="btn btn-outline-secondary" type="button">
              <i class="fa fa-search"></i>
            </button>
          </div>
        </div>
        <!--div class="bg clearfix col-11 btn_new">
          <button type="button" class="btn btn-primary btn-info btn-secondary " aria-haspopup="true" aria-expanded="false" /><a style="color:#ffffff" href="/station/signup">NOVO</a></button>
        </div-->
        <table id="myTable" class="table table-bordered mt-3">
          <tbody class="element">
            <thead bgcolor="#17A2B8">
              <tr>
                <th scope="row" class="font-book text-center ">
                  <font color="white">Matr√≠cula</font>
                </th>
                <th scope="row" class="font-book text-center ">
                  <font color="white">Aparelho</font>
                </th>
                <th scope="row" class="font-book text-center ">
                  <font color="white">Nome</font>
                </th>
                <th scope="row" class="font-book text-center ">
                  <font color="white">Status</font>
                </th>
                <th scope="row" class="font-book text-center ">
                  <font color="white">Editar</font>
                </th>
                <th scope="row" class="font-book text-center ">
                  <font color="white">Perfil</font>
                </th>
              </tr>
            </thead>

            {{#each stations}}
            <tr>
              <th scope="row" class="font-book text-left ">{{codeStation}}</th>
              <th scope="row" class="font-book text-left ">{{idesp}}</th>
              <th scope="row" class="font-book text-left ">{{nameEmployed}}</th>
              <th id="Atualizar" scope="row" class="font-book text-center">{{status}}</th>
              <th width="50" scope="row" style="cursor: pointer;" class="font-book text-center"
                onclick="location.href='/station/edit/{{id}}'"><b>...</b></th>
              <th style="cursor: pointer;" onclick="location.href='/station/logUse/{{id}}'"><img
                  src="https://image.flaticon.com/icons/png/512/68/68917.png" width=20 height=20></th>
            </tr>
            {{/each}}
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>
<br>
<script type="text/javascript" src="/javascripts/edit.js">  </script>